<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserInfoActivity">

    <View
        android:id="@+id/dummy1"
        android:layout_width="365dp"
        android:layout_height="1dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toTopOf="@id/border1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/border1"
        android:layout_width="0dp"
        android:layout_height="145dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dummy1" />

    <TextView
        android:id="@+id/border_label1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="#FFFFFFFF"
        android:text="端末情報"
        app:layout_constraintStart_toStartOf="@id/border1"
        app:layout_constraintTop_toTopOf="@id/dummy1" />

    <TextView
        android:id="@+id/label_policestation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:text="警察署：" />

    <TextView
        android:id="@+id/text_policestation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="104dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/border1" />

    <TextView
        android:id="@+id/label_telephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="電話番号：" />

    <TextView
        android:id="@+id/text_telephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="179dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/text_extension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/label_extension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="内線番号：" />

    <TextView
        android:id="@+id/label_device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="端末番号：" />

    <TextView
        android:id="@+id/text_device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="80dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/text_extension"
        tools:layout_editor_absoluteY="66dp" />

    <View
        android:id="@+id/dummy2"
        android:layout_width="365dp"
        android:layout_height="1dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/border1" />

    <View
        android:id="@+id/border2"
        android:layout_width="0dp"
        android:layout_height="373dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dummy2" />

    <TextView
        android:id="@+id/border_label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="#FFFFFFFF"
        android:text="ユーザー情報"
        app:layout_constraintStart_toStartOf="@id/border2"
        app:layout_constraintTop_toBottomOf="@id/dummy2" />

    <View
        android:id="@+id/dummy3"
        android:layout_width="365dp"
        android:layout_height="1dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="@id/border2"
        app:layout_constraintStart_toStartOf="@id/border2"
        app:layout_constraintTop_toTopOf="@id/border2" />

    <View
        android:id="@+id/border3"
        android:layout_width="0dp"
        android:layout_height="124dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="@id/border2"
        app:layout_constraintStart_toStartOf="@id/border2"
        app:layout_constraintTop_toTopOf="@id/dummy3" />

    <TextView
        android:id="@+id/border_label3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="#FFFFFFFF"
        android:text="入力担当"
        app:layout_constraintStart_toStartOf="@id/border3"
        app:layout_constraintTop_toBottomOf="@id/dummy3" />

    <View
        android:id="@+id/dummy4"
        android:layout_width="365dp"
        android:layout_height="1dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="@id/border2"
        app:layout_constraintStart_toStartOf="@id/border2"
        app:layout_constraintTop_toBottomOf="@id/border3" />

    <View
        android:id="@+id/border4"
        android:layout_width="0dp"
        android:layout_height="154dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="@id/border2"
        app:layout_constraintStart_toStartOf="@id/border2"
        app:layout_constraintTop_toBottomOf="@id/dummy4" />

    <TextView
        android:id="@+id/border_label4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="#FFFFFFFF"
        android:text="印刷担当"
        app:layout_constraintStart_toStartOf="@id/border4"
        app:layout_constraintTop_toBottomOf="@id/dummy4" />

    <Button
        android:id="@+id/button_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>